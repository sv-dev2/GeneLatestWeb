@model InsuranceClaim.Models.ALMRePrintModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_InsuranceMain.cshtml";
}


<div class="body-content-section">
    <div class="container">
        <div class="form-group-section">
            <div class="form-title"><h2>Edit</h2></div>
            @if (TempData["errorMsg"] != null)
            {
                <div class="alert alert-danger">
                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                    <strong>@TempData["errorMsg"]</strong> .
                </div>
            }
            @using (Html.BeginForm("Edit", "City", FormMethod.Post, new { @class = "form-horizontal-center", role = "form" }))
            {
                @Html.HiddenFor(x => x.Id)
                <div class="row">
                    <div class="col-sm-6 col-md-6 form-group-text">
                        <div class="form-group">
                            @Html.LabelFor(x => x.VRN, new { @class = "gender-label" })
                            @Html.TextBoxFor(x => x.VRN, new { @class = "form-control", @placeholder = "VRN" })
                            @Html.ValidationMessageFor(x => x.VRN, "", new { @class = "text-danger" })
                        </div>
                    </div>

                </div>

                <div class="row">
                    <div class="col-sm-6 col-md-6 form-group-text">
                        <div class="form-group">
                            @Html.LabelFor(x => x.OTP, new { @class = "gender-label" })
                            @Html.TextBoxFor(x => x.OTP, new { @class = "form-control", @name = "CityName", @id = "CityName", @placeholder = "Enter CityName" })
                            @Html.ValidationMessageFor(x => x.OTP, "", new { @class = "text-danger" })
                        </div>
                    </div>
                </div> 

            }

        </div>
    </div>
</div>




